,EALA;AAMA,QAAA,KAAA,EAAA;AANA,OADA;AAVA,KAAA;AAqBA,GA3BA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,EAAA,kBAAA;AACA,WAAA,IAAA,CAAA,IAAA,CAAA;AACA,QAAA,MAAA,EAAA,CADA;AAEA,QAAA,IAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA,KAHA;AAIA,QAAA,cAAA,EAAA,EAJA;AAKA,QAAA,KAAA,EAAA,EALA;AAMA,QAAA,OAAA,EAAA,CAAA;AAAA,UAAA,MAAA,EAAA;AAAA,SAAA;AANA,OAAA;AAQA,KAVA;AAWA,IAAA,SAAA,EAAA,mBAAA,KAAA,EAAA;AACA,WAAA,IAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KAbA;AAcA,IAAA,SAAA,EAAA,mBAAA,GAAA,EAAA;AACA,MAAA,GAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAAA;AACA,KAhBA;AAiBA,IAAA,YAAA,EAAA,sBAAA,GAAA,EAAA,GAAA,EAAA;AACA,MAAA,GAAA,CAAA,OAAA,CAAA,MAAA,CAAA,GAAA,EAAA,CAAA;AACA,KAnBA;AAoBA,IAAA,iBApBA,6BAoBA,GApBA,EAoBA;AACA,UAAA,QAAA,GAAA,KAAA,IAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,IAAA,KAAA,GAAA,CAAA,IAAA;AACA,OAFA,CAAA;AAGA,aAAA,QAAA;AACA,KAzBA;AA0BA,IAAA,SA1BA,qBA0BA,GA1BA,EA0BA;AACA,aAAA,GAAA,CAAA,cAAA,KAAA,EAAA;AACA,KA5BA;AA6BA,IAAA,WA7BA,uBA6BA,KA7BA,EA6BA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,KAAA,IAAA,EAAA,KAAA;AACA;AA/BA,GA5BA;AA6DA,EAAA,QAAA,EAAA;AA7DA,CAAA","sourcesContent":["<template>\n  <div id=\"formbuilder\" class=\"container\">\n    <div>Heloo Form Builder</div>\n    <div class=\"col-lg-12\">\n      <div class=\"row  form-row\" v-for=\"(row, index) in rows\" :key=\"index\">\n        <div class=\"col-lg-12\">\n          <div class=\"row\">\n            <div class=\"col-lg-12 row\">\n              <div class=\"col-lg-2\">\n                <select name=\"\" id=\"\" class=\"form-control\" v-model=\"row.select\">\n                  <option\n                    :value=\"option.value\"\n                    v-for=\"(option, key) in options\"\n                    :key=\"key\"\n                    >{{ option.label }}</option\n                  >\n                </select>\n              </div>\n              <div class=\"col-lg-5\">\n                <input\n                  type=\"text\"\n                  class=\"form-control\"\n                  placeholder=\"Question\"\n                  v-model=\"row.name\"\n                />\n              </div>\n              <div class=\"col-lg-2\">\n                <a for=\"\"> Parent Question</a>\n              </div>\n              <div class=\"col-lg-2\">\n                <select\n                  name=\"\"\n                  id=\"parentquestion\"\n                  class=\"form-control\"\n                  v-model=\"row.parentquestion\"\n                >\n                  <option value=\"\"></option>\n                  <option\n                    :value=\"key\"\n                    v-for=\"(row, key) in getParentQuestion(row)\"\n                    :key=\"key\"\n                    >{{ key }}</option\n                  >\n                </select>\n                <!-- <input\n                  type=\"checkbox\"\n                  v-model=\"row.check\"\n                />&nbsp;&nbsp;Mandatory -->\n              </div>\n              <div class=\"col-lg-1\">\n                <button class=\"btn btn-danger\" @click=\"deleteRow(index)\">\n                  X\n                </button>\n              </div>\n            </div>\n          </div>\n          <div\n            class=\"row\"\n            v-if=\"row.select === 'Check box' || row.select == 'Radio button' || row.select == 'Drop down'\"\n          >\n            <div>\n              <div\n                class=\"row subrow\"\n                v-for=\"(subrow, ind) in row.subrows\"\n                :key=\"ind\"\n              >\n                <div class=\"col-lg-12 row\">\n                  <div class=\"col-lg-7\">\n                    <input\n                      type=\"text\"\n                      class=\"form-control\"\n                      v-model=\"subrow.answer\"\n                      placeholder=\"Answer\"\n                    />\n                  </div>\n                  <div class=\"col-lg-2\">\n                    <button\n                      class=\"btn btn-danger\"\n                      @click=\"deleteSubrow(row, ind)\"\n                    >\n                      X\n                    </button>\n                  </div>\n                </div>\n              </div>\n              <div class=\"row\">\n                <div class=\"col-lg-12\">\n                  <button\n                    class=\"btn btn-success center-block\"\n                    @click=\"addSubRow(row)\"\n                  >\n                    Add Answer\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div v-if=\"showRules(row)\">\n            <Rules @update=\"processRule\" :wizard-data=\"row\" />\n          </div>\n        </div>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-lg-12\">\n          <button type=\"submit\" class=\"btn btn-info\">Submit</button>\n          <button type=\"submit\" class=\"btn btn-success\" @click=\"addRow\">\n            Add Row\n          </button>\n        </div>\n      </div>\n      <!-- <div><pre><code>{{rows}}</code></pre></div> -->\n      \n    </div>\n  </div>\n</template>\n\n<script>\nimport Rules from \"./FormRules\";\nexport default {\n  name: \"formbuilder\",\n  components: {\n    Rules\n  },\n  data() {\n    return {\n      options: [\n        { label: \"Text\", value: \"Text\" },\n        { label: \"Numeric\", value: \"Numeric\" },\n        { label: \"Check box\", value: \"Check box\" },\n        { label: \"Radio button\", value: \"Radio button\" },\n        { label: \"Drop down\", value: \"Drop down\" },\n        { label: \"Image\", value: \"Image\" },\n        { label: \"Date\", value: \"Date\" }\n      ],\n      rows: [\n        {\n          select: \"Text\",\n          name: \"\",\n          check: false,\n          subrows: [{ answer: \"\" }],\n          parentquestion: \"\",\n          rules: {}\n        }\n      ]\n    };\n  },\n  methods: {\n    addRow: function() {\n      this.rows.push({\n        select: 1,\n        name: \"\",\n        check: false,\n        parentquestion: \"\",\n        rules: {},\n        subrows: [{ answer: \"\" }]\n      });\n    },\n    deleteRow: function(index) {\n      this.rows.splice(index, 1);\n    },\n    addSubRow: function(row) {\n      row.subrows.push({ answer: \"\" });\n    },\n    deleteSubrow: function(row, ind) {\n      row.subrows.splice(ind, 1);\n    },\n    getParentQuestion(row) {\n      const filtered = this.rows.filter(function(item) {\n        return item.name !== row.name;\n      });\n      return filtered;\n    },\n    showRules(row) {\n      return row.parentquestion !== \"\";\n    },\n    processRule(rules) {\n      Object.assign(this.form, rules);\n    }\n  },\n  computed: {}\n};\n</script>\n<style>\n.form-row {\n  border: 1px solid #e2e2e2;\n  margin: 10px;\n  padding: 20px;\n  background: #f2f2f2;\n}\n.subrow {\n  margin: 5px;\n  padding: 5px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}