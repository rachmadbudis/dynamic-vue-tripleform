LA;AAMA,QAAA,MAAA,EAAA,EANA;AAOA,QAAA,MAAA,EAAA,EAPA;AAQA,QAAA,MAAA,EAAA;AARA,OADA;AATA,KAAA;AAsBA,GA5BA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,EAAA,kBAAA;AACA,WAAA,SAAA,CAAA,IAAA,CAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,GAAA,EAAA,EAFA;AAGA,QAAA,SAAA,EAAA,EAHA;AAIA,QAAA,KAAA,EAAA,IAJA;AAKA,QAAA,OAAA,EAAA,CAAA;AAAA,UAAA,MAAA,EAAA;AAAA,SAAA,CALA;AAMA,QAAA,MAAA,EAAA,EANA;AAOA,QAAA,MAAA,EAAA,EAPA;AAQA,QAAA,MAAA,EAAA;AARA,OAAA;AAUA,KAZA;AAaA,IAAA,SAAA,EAAA,mBAAA,KAAA,EAAA;AACA,WAAA,SAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KAfA;AAgBA,IAAA,SAAA,EAAA,mBAAA,GAAA,EAAA;AACA,MAAA,GAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAAA;AACA,KAlBA;AAmBA,IAAA,YAAA,EAAA,sBAAA,GAAA,EAAA,GAAA,EAAA;AACA,MAAA,GAAA,CAAA,OAAA,CAAA,MAAA,CAAA,GAAA,EAAA,CAAA;AACA,KArBA;AAsBA,IAAA,iBAtBA,6BAsBA,GAtBA,EAsBA;AACA,UAAA,QAAA,GAAA,KAAA,SAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,GAAA,KAAA,GAAA,CAAA,GAAA;AACA,OAFA,CAAA;AAGA,aAAA,QAAA;AACA,KA3BA;AA4BA,IAAA,SA5BA,qBA4BA,GA5BA,EA4BA;AACA,aAAA,GAAA,CAAA,SAAA,KAAA,EAAA;AACA,KA9BA;AA+BA,IAAA,WA/BA,uBA+BA,KA/BA,EA+BA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,KAAA,IAAA,EAAA,KAAA;AACA,KAjCA;AAkCA,IAAA,eAlCA,2BAkCA,GAlCA,EAkCA;AACA,UAAA,GAAA,CAAA,GAAA,KAAA,EAAA,EAAA;AACA,eAAA,IAAA;AACA;AACA;AAtCA,GA7BA;AAqEA,EAAA,WAAA,EAAA;AACA,IAAA,SAAA,EAAA,CACA;AACA,MAAA,MAAA,EAAA;AACA,QAAA,QAAA,EAAA;AADA,OADA;AAIA,MAAA,GAAA,EAAA;AACA,QAAA,QAAA,EAAA;AADA,OAJA;AAOA,MAAA,MAAA,EAAA;AACA,QAAA,QAAA,EAAA;AADA;AAPA,KADA;AADA;AArEA,CAAA","sourcesContent":["<template>\n  <div id=\"formbuilder\" class=\"container\">\n    <div>Heloo Form Builder</div>\n    <div class=\"col-lg-12\">\n      <div class=\"row  form-row\" v-for=\"(row, index) in questions\" :key=\"index\">\n        <div class=\"col-lg-12\">\n          <div class=\"row\">\n            <div class=\"col-lg-12 row\">\n              <div class=\"col-lg-2\">\n                <select name=\"\" id=\"\" class=\"form-control\" v-model=\"row.select\">\n                  <option\n                    :value=\"option.value\"\n                    v-for=\"(option, key) in options\"\n                    :key=\"key\"\n                    >{{ option.label }}</option\n                  >\n                </select>\n              </div>\n              <div class=\"col-lg-5\">\n                <input\n                  type=\"text\"\n                  class=\"form-control\"\n                  placeholder=\"Question\"\n                  v-model=\"row.key\"\n                />\n              </div>\n              <div class=\"col-lg-2\">\n                <a for=\"\"> Parent Question</a>\n              </div>\n              <div class=\"col-lg-2\">\n                <select\n                  name=\"\"\n                  id=\"parentKey\"\n                  class=\"form-control\"\n                  v-model=\"row.parentKey\"\n                >\n                  <option value=\"\"></option>\n                  <option\n                    :value=\"key\"\n                    v-for=\"(row, key) in getParentQuestion(row)\"\n                    :key=\"key\"\n                    >{{ key }}</option\n                  >\n                </select>\n                <!-- <input\n                  type=\"checkbox\"\n                  v-model=\"row.check\"\n                />&nbsp;&nbsp;Mandatory -->\n              </div>\n              <div class=\"col-lg-1\">\n                <button class=\"btn btn-danger\" @click=\"deleteRow(index)\">\n                  X\n                </button>\n              </div>\n            </div>\n          </div>\n          <div class=\"row translation-div\" v-if=\"showTranslation(row)\">\n            <div class=\"col-lg-4 row\">\n              <label class=\"text-center\" for=\"\"> NL Translation</label>\n              <input\n                class=\"form-control\"\n                v-model=\"row.key_nl\"\n                type=\"text\"\n                placeholder=\"NL Translations\"\n              />\n            </div>\n            <div class=\"col-lg-4 row\">\n              <label for=\"\"> AR Translation</label>\n              <input\n                class=\"form-control\"\n                v-model=\"row.key_ar\"\n                type=\"text\"\n                placeholder=\"AR Translations\"\n              />\n            </div>\n            <div class=\"col-lg-4 row\">\n              <label for=\"\"> EN Translation</label>\n              <input\n                class=\"form-control\"\n                v-model=\"row.key_en\"\n                type=\"text\"\n                placeholder=\"EN Translations\"\n              />\n            </div>\n          </div>\n          <div\n            class=\"row\"\n            v-if=\"\n              row.select === 'Check box' ||\n                row.select == 'Radio button' ||\n                row.select == 'Drop down'\n            \"\n          >\n            <div>\n              <div\n                class=\"row subrow\"\n                v-for=\"(subrow, ind) in row.options\"\n                :key=\"ind\"\n              >\n                <div class=\"col-lg-12 row\">\n                  <div class=\"col-lg-7\">\n                    <input\n                      type=\"text\"\n                      class=\"form-control\"\n                      v-model=\"subrow.answer\"\n                      placeholder=\"Answer\"\n                    />\n                  </div>\n                  <div class=\"col-lg-2\">\n                    <button\n                      class=\"btn btn-danger\"\n                      @click=\"deleteSubrow(row, ind)\"\n                    >\n                      X\n                    </button>\n                  </div>\n                </div>\n              </div>\n              <div class=\"row\">\n                <div class=\"col-lg-12\">\n                  <button\n                    class=\"btn btn-success center-block\"\n                    @click=\"addSubRow(row)\"\n                  >\n                    Add Answer\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div v-if=\"showRules(row)\">\n            <Rules @update=\"processRule\" :wizard-data=\"row\" />\n          </div>\n        </div>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-lg-12\">\n          <button type=\"submit\" class=\"btn btn-info\">Submit</button>\n          <button type=\"submit\" class=\"btn btn-success\" @click=\"addRow\">\n            Add Row\n          </button>\n        </div>\n      </div>\n      <!-- <div><pre><code>{{rows}}</code></pre></div> -->\n    </div>\n  </div>\n</template>\n\n<script>\nimport Rules from \"./FormRules\";\nimport { required } from \"vuelidate/lib/validators\";\n\nexport default {\n  name: \"formbuilder\",\n  components: {\n    Rules\n  },\n  data() {\n    return {\n      options: [\n        { label: \"Text\", value: \"Text\" },\n        { label: \"Numeric\", value: \"Numeric\" },\n        { label: \"Check box\", value: \"Check box\" },\n        { label: \"Radio button\", value: \"Radio button\" },\n        { label: \"Drop down\", value: \"Drop down\" },\n        { label: \"Date\", value: \"Date\" }\n      ],\n      questions: [\n        {\n          select: \"Text\",\n          key: \"\",\n          options: [{ answer: \"\" }],\n          parentKey: \"\",\n          rules: null,\n          key_ar: \"\",\n          key_en: \"\",\n          key_nl: \"\"\n        }\n      ]\n    };\n  },\n  methods: {\n    addRow: function() {\n      this.questions.push({\n        select: \"Text\",\n        key: \"\",\n        parentKey: \"\",\n        rules: null,\n        options: [{ answer: \"\" }],\n        key_ar: \"\",\n        key_en: \"\",\n        key_nl: \"\"\n      });\n    },\n    deleteRow: function(index) {\n      this.questions.splice(index, 1);\n    },\n    addSubRow: function(row) {\n      row.options.push({ answer: \"\" });\n    },\n    deleteSubrow: function(row, ind) {\n      row.options.splice(ind, 1);\n    },\n    getParentQuestion(row) {\n      const filtered = this.questions.filter(function(item) {\n        return item.key !== row.key;\n      });\n      return filtered;\n    },\n    showRules(row) {\n      return row.parentKey !== \"\";\n    },\n    processRule(rules) {\n      Object.assign(this.form, rules);\n    },\n    showTranslation(row) {\n      if (row.key !== \"\") {\n        return true;\n      }\n    }\n  },\n  validations: {\n    questions: [\n      {\n        select: {\n          required\n        },\n        key: {\n          required\n        },\n        key_en:{\n          required\n        }\n      }\n    ]\n  }\n};\n</script>\n<style>\n.form-row {\n  border: 1px solid #e2e2e2;\n  margin: 10px;\n  padding: 20px;\n  background: #f2f2f2;\n}\n.subrow {\n  margin: 5px;\n  padding: 5px;\n}\n.text-center {\n  text-align: center;\n}\n.translation-div {\n  margin-top: 15px;\n  margin-left: 15px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}